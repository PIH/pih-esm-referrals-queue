{
  "name": "@pih/esm-referrals-queue",
  "version": "1.0.0",
  "description": "A page for viewing the J9 Home Visits from CommCare",
  "homepage": "https://pihemr.atlassian.net/wiki/spaces/DSS/pages/538116104/PIH+EMR+Referrals+Queue",
  "main": "dist/pih-esm-referrals-queue.js",
  "directories": {
    "lib": "dist"
  },
  "scripts": {
    "analyze": "webpack --mode=production --env.analyze=true",
    "build": "webpack --mode=production",
    "coverage": "jest --coverage",
    "lint": "eslint src --ext js,jsx,ts,tsx",
    "prettier": "prettier --write './**'",
    "autodoc": "config-autodoc",
    "start": "webpack-dev-server",
    "test": "jest --passWithNoTests",
    "typescript": "tsc",
    "update-translations": "tx pull",
    "watch-tests": "jest --watch",
    "prepublishOnly": "yarn build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && concurrently 'yarn:lint' 'yarn:test' 'yarn:typescript' 'yarn:autodoc' 'yarn:update-translations' && git add README.md translations/*.json"
    }
  },
  "types": "src/pih-esm-referrals-queue.d.tsx",
  "browserslist": [
    "extends browserslist-config-openmrs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pih/esm-referrals-queue.git"
  },
  "bugs": {
    "url": "https://github.com/pih/esm-referrals-queue/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@openmrs/config-cli": "^1.0.0",
    "@openmrs/react-root-decorator": "^3.2.0",
    "@testing-library/jest-dom": "^5.0.2",
    "@testing-library/react": "^9.4.0",
    "@types/jest": "^24.9.1",
    "@types/moment": "^2.13.0",
    "@types/react-dates": "^17.1.10",
    "@typescript-eslint/parser": "^2.18.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "browserslist-config-openmrs": "^1.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.1.0",
    "coveralls": "^3.0.7",
    "css-loader": "^3.4.2",
    "dayjs": "^1.8.23",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.6.0",
    "eslint-config-ts-react-important-stuff": "^2.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "fork-ts-checker-webpack-plugin": "^1.6.0",
    "husky": "^3.0.9",
    "i18next": "^19.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "lint-staged": "^9.4.3",
    "mockdate": "^2.0.5",
    "moment": "^2.24.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^1.11.1",
    "react-dates": "^21.8.0",
    "react-i18next": "^11.3.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "single-spa-react": "^2.11.0",
    "style-loader": "^1.1.3",
    "systemjs-webpack-interop": "^1.1.2",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-config-single-spa-react": "^1.3.0",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@openmrs/esm-api": "^1.2.2",
    "@openmrs/esm-error-handling": "^1.1.0",
    "@openmrs/esm-module-config": "^1.1.1",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/systemjs": "^6.1.0",
    "@types/webpack-env": "^1.15.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "rxjs": "^6.5.4"
  }
}
