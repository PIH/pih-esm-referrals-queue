{
  "name": "@pih/esm-referrals-queue-app",
  "version": "1.0.5",
  "description": "A page for viewing the J9 Home Visits from CommCare",
  "main": "dist/pih-esm-referrals-queue-app.js",
  "types": "src/pih-esm-referrals-queue.d.tsx",
  "license": "MPL-2.0",
  "homepage": "https://pihemr.atlassian.net/wiki/spaces/DSS/pages/538116104/PIH+EMR+Referrals+Queue",
  "scripts": {
    "start": "openmrs debug --run-project --backend https://humci.pih-emr.org/ --api-url /mirebalais/",
    "serve": "webpack-dev-server",
    "build": "webpack --mode production",
    "analyze": "webpack --mode=production --env.analyze=true",
    "lint": "eslint src --ext tsx",
    "prettier": "prettier 'src/**/*' --write",
    "typescript": "tsc",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "coverage": "npm test -- --coverage",
    "update-translations": "tx pull"
  },
  "browserslist": [
    "extends browserslist-config-openmrs"
  ],
  "keywords": [
    "openmrs"
  ],
  "publishConfig": {
    "access": "public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && concurrently 'npm:lint' 'npm:test' 'npm:typescript' 'npm:update-translations' && git add translations/*.json"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pih/esm-referrals-queue.git"
  },
  "bugs": {
    "url": "https://github.com/pih/esm-referrals-queue/issues"
  },
  "dependencies": {
    "systemjs-webpack-interop": "^1.1.2"
  },
  "peerDependencies": {
    "@openmrs/esm-framework": "3.x",
    "dayjs": "^1.x",
    "react": "16.x",
    "react-dom": "16.x",
    "react-i18next": "^11.x"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@openmrs/esm-framework": "next",
    "@testing-library/jest-dom": "^5.0.2",
    "@testing-library/react": "^11.2.6",
    "@types/jest": "^24.9.1",
    "@types/moment": "^2.13.0",
    "@types/react": "^16.9.19",
    "@types/react-dates": "^17.1.10",
    "@types/react-dom": "^16.9.5",
    "@types/systemjs": "^6.1.0",
    "@types/webpack-env": "^1.15.1",
    "@typescript-eslint/parser": "^2.18.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "browserslist-config-openmrs": "^1.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.1.0",
    "coveralls": "^3.0.7",
    "css-loader": "^3.4.2",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.6.0",
    "eslint-config-ts-react-important-stuff": "^2.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "fork-ts-checker-webpack-plugin": "^1.6.0",
    "husky": "^3.0.9",
    "i18next": "^19.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^9.4.3",
    "mockdate": "^2.0.5",
    "moment": "^2.24.0",
    "openmrs": "next",
    "prettier": "^1.19.1",
    "pretty-quick": "^1.11.1",
    "react-dates": "^21.8.0",
    "react-i18next": "^11.3.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "rxjs": "^6.5.4",
    "style-loader": "^1.1.3",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5",
